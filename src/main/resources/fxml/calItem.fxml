<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="calendarItemPane" prefHeight="40.0" prefWidth="285.0" style="-fx-background-color: #f2f2f2; -fx-border-radius: 5px;" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1"
onMouseClicked="#handleSelect" fx:controller="controller.CalItemController">
   <children>
      <CheckBox fx:id="taskCheckBox" layoutX="8.0" layoutY="5.0" mnemonicParsing="false" onAction="#handleCheck">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
